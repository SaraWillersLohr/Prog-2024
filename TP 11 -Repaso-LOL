#include <bits/stdc++.h>
using namespace std;

string Matrix(string numeros[][100], int filas, int columnas);

int main() {
    string numeros[100][100];
    int filas, columnas;

    cout << "Digite el numero de filas: " << endl;
    cin >> filas;
    cout << "Digite el numero de columnas: " << endl;
    cin >> columnas;

    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < columnas; j++) {
            cout << "Digite un numero [" << i << "][" << j << "]: " << endl;
            cin >> numeros[i][j];
        }
    }

    cout << Matrix(numeros, filas, columnas) << endl;

    return 0;
}

string Matrix(string numeros[][100], int filas, int columnas) {
    string resultado = "";

    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < columnas; j++) {
            resultado += numeros[i][j] + " ";
        }
        resultado += "\n";
    }

    return resultado;
}
